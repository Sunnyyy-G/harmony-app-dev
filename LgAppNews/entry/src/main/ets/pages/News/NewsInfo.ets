class INews {
  /**
   * 标题
   */
  title: string
  /**
   * 作者
   */
  author: string
  /**
   * 时间
   */
  time: string
  /**
   * 地点
   */
  area: string
  /**
   * 首图
   */
  firstImage: string | Resource
  /**
   * 跟帖数
   */
  msgCount: number
  /**
   * 具体新闻内容
   */
  content: string

  constructor(title: string, author: string, time: string, area: string, firstImage: string | Resource,
    msgCount: number, content: string) {
    this.title = title
    this.author = author
    this.time = time
    this.area = area
    this.firstImage = firstImage
    this.msgCount = msgCount
    this.content = content
  }
}

